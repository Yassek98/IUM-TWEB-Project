{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch Express_server",
            "request": "launch",
            "program": "${workspaceFolder}/solution/backend/express_server/bin/www",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "console": "integratedTerminal"
        },
        {
            "name": "Launch Chrome with Debugger",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/solution/backend/express_server",
            "sourceMaps": true,
            "trace": true
        }

    ]
}