{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Express_server",
            "request": "launch",
            "program": "${workspaceFolder}/solution/backend/express_server/bin/www",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "console": "integratedTerminal"
        },
        {
            "name": "Launch Chrome 3000",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/solution/backend/express_server",
            "sourceMaps": true,
            "trace": true
        },
        {
            "type": "java",
            "name": "Launch SpringBoot_server",
            "request": "launch",
            "mainClass": "com.twebproject.appfilm.AppfilmApplication",
            "projectName": "springboot_server",
            "vmArgs": "-Dspring-boot.run.profiles=dev",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Open Chrome 8080",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }
    ]
}